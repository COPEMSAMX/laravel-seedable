{
  "name": "gregoriohc/laravel-seedable",
  "description": "Laravel package that allows seeding based on a trait and custom seeder",
  "keywords": ["laravel", "package", "seed", "seeding", "database"],
  "homepage": "https://github.com/gregoriohc/laravel-seedable",
  "license": "MIT",
  "authors": [
    {
      "name": "Gregorio Hern√°ndez Caso",
      "email": "gregoriohc@gmail.com"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "illuminate/support": "~5.5",
    "illuminate/database": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Gregoriohc\\Seedable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gregoriohc\\Seedable\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gregoriohc\\Seedable\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
